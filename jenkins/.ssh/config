Host *
    ServerAliveInterval 300

Host khanacademy.kilnhg.com
    IdentityFile ~/.ssh/id_rsa.ReadWriteKiln
    # This helps make sure kiln doesn't hang up on us on long pulls.
    ServerAliveInterval 60

Host github.com-jenkins
    HostName github.com
    IdentityFile ~/.ssh/id_rsa.ReadWriteKiln
    ServerAliveInterval 60
